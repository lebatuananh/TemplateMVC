@model IntroduceConfigDto
@inject IViewLocalizer Localizer
@{
    ViewData["Title"] = Localizer["factory"];
}
<div id="breacrumb">
    <div class="container">
        <ol class="breadcrumb">

            <li>
                <a href="/gioi-thieu.html">@Localizer["introduce"]</a>
            </li>
            <li class="active">@Localizer["factory"]</li>
        </ol>
    </div>
</div>
<!-- END -->
<div id="about-page" class="wow fadeInUp" data-wow-duration="1s">
    <div class="container">
        <div class="row fl-wrap">
            <div class="col-md-3 col-sm-4 col-xs-12">
                <h2 class="title-head tt no-mg">
                    @Localizer["factory"]
                </h2>
            </div>
            <div class="col-xs-12 col-sm-8 col-md-9">
                <div class="ap-content-tt">
                    @Html.Raw(Model.Content)
                </div>
            </div>
        </div>

    </div>
</div>
<!-- END -->
